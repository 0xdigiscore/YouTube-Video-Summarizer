{
    "manifest_version": 2,
    "name": "YouTube Video Summarizer",
    "version": "1.0.0",
    "description": "A Chrome extension to summarize YouTube videos using OpenAI API",
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "logo192.png"
    },
    "permissions": ["activeTab", "https://www.googleapis.com/*"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
  